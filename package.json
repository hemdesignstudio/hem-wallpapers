{
  "name": "hem-wallpapers",
  "productName": "Hem Wallpapers",
  "description": "A breathtaking photo for your desktop wallpaper, every day",
  "version": "1.0.1",
  "main": "main.js",
  "author": "David Paulsson <david@hem.com>",
  "repository": "https://github.com/hemdesignstudio/hem-wallpapers",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development electron .",
    "pack": "electron-builder --dir",
    "build": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.hem.wallpapers",
    "mac": {
      "category": "public.app-category.utilities",
      "extendInfo": {
        "LSUIElement": "1"
      }
    }
  },
  "devDependencies": {
    "electron": "^17.0.1",
    "electron-builder": "^22.14.13"
  },
  "dependencies": {
    "contentful": "^9.1.10",
    "menubar": "^9.1.1",
    "wallpaper": "^5"
  }
}
